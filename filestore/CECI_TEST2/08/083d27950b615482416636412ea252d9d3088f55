isCloser=p[dist]<distance[k],isAbove=p[dist]===distance[k]&&p.series.group.zIndex>=kdpoint[k].series.group.zIndex;if(isCloser||isAbove){distance[k]=p[dist];kdpoint[k]=p;}}});}});}
