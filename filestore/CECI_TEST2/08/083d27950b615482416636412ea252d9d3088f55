if(outOfBounds){touch0Now-=0.8*(touch0Now-lastValidTouch[xy][0]);if(!singleTouch){touch1Now-=0.8*(touch1Now-lastValidTouch[xy][1]);}
setScale();}else{lastValidTouch[xy]=[touch0Now,touch1Now];}
