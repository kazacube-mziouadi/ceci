dhtmlXGridObject.prototype._get_filters=function(value,type){var fs=value.split(',');var filters=[];for(var i=0;i<fs.length;i++){if(fs[i]=='')continue;var f=this['_get_'+type+'_filter'](fs[i]);filters.push(f);}
