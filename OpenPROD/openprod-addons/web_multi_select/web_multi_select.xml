<?xml version="1.0" encoding="utf-8"?>

<openerp>
    <data>
		    
        <template id="assets_backend_multiple_select" name="multiple select assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/web_multi_select/static/src/js/multiple-select.js"></script>
            </xpath>
        </template>

        <template id="assets_backend_multiple_select_list_view" name="multiple select list view assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <script type="text/javascript" src="/web_multi_select/static/src/js/list_view.js"></script>
            </xpath>
        </template>

        <template id="assets_backend_multiple_select_css" name="multiple select css assets" inherit_id="web.assets_backend">
            <xpath expr="." position="inside">
                <link rel="stylesheet" href="/web_multi_select/static/src/css/multiple-select.css"/>
            </xpath>
        </template>

    </data>
</openerp>
