<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data noupdate="1">
        <!-- ============= -->
        <!-- STOCK PICKING -->
        <!-- ============= -->
        
        <!-- IN -->
        <record id="seq_picking_in" model="ir.sequence">
            <field name="name">Picking IN</field>
            <field name="code">stock.picking.in</field>
            <field name="prefix">IN/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- OUT -->
        <record id="seq_picking_out" model="ir.sequence">
            <field name="name">Picking OUT</field>
            <field name="code">stock.picking.out</field>
            <field name="prefix">OUT/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- ========= -->
        <!-- STOCK LOT -->
        <!-- ========= -->
        <record id="seq_lot" model="ir.sequence">
            <field name="name">Lot</field>
            <field name="code">stock.lot</field>
            <field name="prefix"></field>
            <field name="padding">16</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- ================= -->
        <!-- PROCUREMENT ORDER -->
        <!-- ================= -->
        <record id="seq_procurement_order" model="ir.sequence">
            <field name="name">Procurement order</field>
            <field name="code">procurement.order</field>
            <field name="prefix">PR/</field>
            <field name="padding">7</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</openerp>
